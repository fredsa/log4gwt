<module>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />

  <!-- Inherit log4gwt and gwt-log support; set log level -->
  <inherits name="com.allen_sauer.gwt.log4gwt.log4gwt" />
  <inherits name="com.allen_sauer.gwt.log.gwt-log" />
  <extend-property name="log_level" values="DEBUG" />
  <set-property name="log_level" value="DEBUG" />

  <entry-point class="com.allen_sauer.gwt.log4gwt.demo.client.Log4gwtDemo" />

  <stylesheet src="Log4gwtDemo.css" />

</module>
